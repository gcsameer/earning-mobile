{
  "expo": {
    "name": "NepEarn",
    "slug": "nepearn",
    "version": "1.0.0",
    "orientation": "portrait",
    "userInterfaceStyle": "dark",
    "splash": {
      "resizeMode": "contain",
      "backgroundColor": "#0f172a"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.nepearn.app"
    },
    "android": {
      "versionCode": 1,
      "adaptiveIcon": {
        "backgroundColor": "#0f172a"
      },
      "package": "com.nepearn.app",
      "permissions": [
        "INTERNET",
        "ACCESS_NETWORK_STATE"
      ]
    },
    "web": {
    },
    "extra": {
      "apiBaseUrl": "https://earning-backend-production.up.railway.app",
      "eas": {
        "projectId": "daad5926-5862-416d-af68-48b403617bb7"
      }
    },
    "plugins": [
      [
        "expo-build-properties",
        {
          "android": {
            "compileSdkVersion": 34,
            "targetSdkVersion": 34,
            "minSdkVersion": 21,
            "buildToolsVersion": "34.0.0"
          }
        }
      ]
    ],
    "owner": "breakout8848s-organization"
  }
}
